      </span><span class="nl">"total_spent_currency"</span><span class="p">:</span><span class="w"> </span><span class="s2">"USD"</span><span class="p">,</span><span class="w">
      </span><span class="nl">"updated_at"</span><span class="p">:</span><span class="w"> </span><span class="s2">"2013-03-11T09:14:11-03:00"</span><span class="p">,</span><span class="w">
      </span><span class="nl">"default_address"</span><span class="p">:</span><span class="w"> </span><span class="p">{</span><span class="w">
